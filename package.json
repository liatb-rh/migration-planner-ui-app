{
  "name": "migration-assessment",
  "private": false,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "build": "fec build",
    "clean": "rimraf node_modules dist .cache",
    "copy-static": "cp -r public/* dist/",
    "lint": "eslint --cache --cache-location node_modules/.cache/eslintcache --cache-strategy content .",
    "format": "prettier --cache --cache-location node_modules/.cache/prettiercache --cache-strategy content .",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "patch:hosts": "fec patch-etc-hosts",
    "start": "HOT=true fec dev --clouddotEnv=stage --uiEnv=stable",
    "start:dev-proxy": "fec dev-proxy --clouddotEnv=stage --uiEnv=stable",
    "start:federated": "fec static",
    "postinstall": "ts-patch install && rimraf .cache",
    "start:standalone": "webpack serve --config webpack.standalone.js",
    "build:standalone": "webpack --config webpack.standalone.js --mode production",
    "serve:standalone": "serve dist-standalone -p 3001"
  },
  "dependencies": {
    "@emotion/css": "^11.13.0",
    "@migration-planner-ui/api-client": "^0.0.37",
    "@migration-planner-ui/ioc": "^0.0.36",
    "@patternfly/react-charts": "7.4.9",
    "@patternfly/react-core": "^6.4.0",
    "@patternfly/react-icons": "^6.4.0",
    "@patternfly/react-table": "^6.4.0",
    "@patternfly/react-tokens": "^6.4.0",
    "@redhat-cloud-services/frontend-components": "^7.0.26",
    "@redhat-cloud-services/frontend-components-notifications": "^6.1.27",
    "@redhat-cloud-services/frontend-components-utilities": "^7.0.22",
    "github-markdown-css": "^5.8.1",
    "html2canvas-pro": "^1.6.0",
    "humanize-plus": "^1.8.2",
    "jspdf": "^4.0.0",
    "lodash": "^4.17.23",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.30.0",
    "react-syntax-highlighter": "^16.1.0",
    "react-use": "^17.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^3.0.14",
    "@redhat-cloud-services/frontend-components-config": "^6.7.4",
    "@redhat-cloud-services/tsc-transform-imports": "^1.0.26",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/humanize-plus": "^1.8.2",
    "@types/node": "^24.0.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitest/coverage-v8": "^4.0.18",
    "css-loader": "^7.1.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^17.1.0",
    "jsdom": "^27.3.0",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "sass-loader": "^16.0.6",
    "style-loader": "^4.0.0",
    "ts-patch": "^3.3.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vitest": "^4.0.16",
    "webpack": "^5.104.1",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  },
  "overrides": {
    "@redhat-cloud-services/frontend-components-config": {
      "webpack-dev-server": ">=5.2.1"
    },
    "@redhat-cloud-services/tsc-transform-imports": {
      "glob": ">=10.4.6"
    }
  },
  "insights": {
    "appname": "assisted-migration-app"
  }
}
